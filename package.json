{
  "name": "decky-capydeploy",
  "version": "0.6.2",
  "description": "Decky plugin for CapyDeploy - receive games and create shortcuts in gaming mode",
  "type": "module",
  "scripts": {
    "build": "shx rm -rf dist && rollup -c --environment ROLLUP_ENV:production",
    "watch": "rollup -c -w --environment ROLLUP_ENV:development"
  },
  "repository": "github:lobinuxsoft/decky-capydeploy",
  "keywords": [
    "decky",
    "plugin",
    "steam-deck",
    "capydeploy",
    "shortcuts"
  ],
  "author": "lobinuxsoft",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@decky/rollup": "^1.0.2",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "18.2.0",
    "rollup": "^4.20.0",
    "rollup-plugin-external-globals": "^0.11.0",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@decky/api": "^1.1.3",
    "@decky/ui": "^4.11.1",
    "react-icons": "5.4.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
